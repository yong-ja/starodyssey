<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <system.diagnostics>
    <sources>
      <source name="Odyssey" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <!--<add name="wpf"/>-->
        </listeners>
      </source>
      <source name="TrackerRaw" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="TrackerRaw"/>
        </listeners>
      </source>
      <source name="TrackerDesc" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="TrackerDesc"/>
        </listeners>
      </source>
      <source name="TrackerBoxPerf" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="TrackerBoxPerf"/>
        </listeners>
      </source>
      <source name="TrackerBoxData" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="TrackerBoxData"/>
        </listeners>
      </source>

      <source name="TrackerPointPerf" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="TrackerPointPerf"/>
        </listeners>
      </source>

      <source name="Network" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="console"/>
        </listeners>
      </source>
      <source name="Rendering" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="console"/>
          <!--<add name="logger"/>-->
        </listeners>
      </source>
      <source name="UI" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <remove name="Default"/>
          <add name="console"/>
          <!--<add name="logger"/>-->
        </listeners>
      </source>
    </sources>
    <switches>
      <!-- You can set the level at which tracing is to occur -->
      <add name="SourceSwitch" value="Information"/>
      <add name="VerboseSwitch" value="Verbose"/>
      <!-- You can turn tracing off -->
      <!--add name="SourceSwitch" value="Off" -->
    </switches>
    <sharedListeners>
      <add name="console" type="AvengersUtd.Odyssey.Utils.Logging.ConsoleTraceListener, Odyssey" initializeData="true"/>
      <!--<add name="logger" type="AvengersUtd.Odyssey.Utils.Logging.LoggerTraceListener, Odyssey" initializeData="false"/>-->
      <!--<add name="wpf" type="AvengersUtd.Odyssey.Utils.Logging.WpfTextTraceListener, Odyssey" initializeData="false"/>-->
      <add name="TrackerRaw" type="WpfTest.TrialStringBuilderListener, WpfTest" initializeData="Raw.csv"/>
      <add name="TrackerDesc" type="WpfTest.TrialStringBuilderListener, WpfTest" initializeData="Desc.csv"/>
      <add name="TrackerBoxPerf" type="WpfTest.TaskFileTraceListener, WpfTest" initializeData="BoxPerf.csv"/>
      <add name="TrackerBoxData" type="WpfTest.TrialFileTraceListener, WpfTest" initializeData="BoxData.csv"/>
      <add name="TrackerPointPerf" type="WpfTest.TaskFileTraceListener, WpfTest" initializeData="PointPerf.csv"/>
      
    </sharedListeners>
    <trace autoflush="false" indentsize="4">
      <listeners>
        <remove name="Default"/>
        <add name="console"/>
        <!--<add name="logger"/>-->
        <!--<add name="wpf"/>-->
        <add name="TrackerRaw"/>
        <add name="TrackerDesc"/>
        <add name="TrackerBoxPerf"/>
        <add name="TrackerBoxData"/>
        <add name="TrackerPointPerf"/>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
