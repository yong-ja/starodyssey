<?xml version="1.0" encoding="utf-8"?>
<ArrayOfControlDescription xmlns:Style="http://www.avengersutd.com">
  <ControlDescription Name="Button" Shape="RectangleMesh" Size="Width:200 Height:50" BorderStyle="Raised" BorderSize="1" TextDescriptionClass="Button">
    <BorderShaders>
      <Border Name="RaisedTL" Color="49FFFFFF" Borders="Top Left" />
      <Border Name="RaisedBR" Color="49000000" Borders="Right Bottom" />
    </BorderShaders>
    <Enabled>
      <LinearGradient Color="FF000000" GradientType="Uniform"/>
      <RadialGradient GradientType="Radial" Center="0.5,0.65" RadiusX="0.75" RadiusY="0.6">
        <Gradient>
          <GradientStop Color="#FFFF0000" Offset="0"/>
          <GradientStop Color="#FFFFFF00" Offset="0.25"/>
          <GradientStop Color="#FF00FF00" Offset="0.5"/>
          <GradientStop Color="#FF0000FF" Offset="0.75"/>
          <GradientStop Color="#FF000000" Offset="1"/>
        </Gradient>
      </RadialGradient>
   
    </Enabled>
    <ColorArray>
      <Color Type="Enabled" Name="RoyalBlue" HexValue="FF4169E1" />
