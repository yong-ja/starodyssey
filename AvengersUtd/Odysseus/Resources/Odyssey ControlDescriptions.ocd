      <Color Type="Focused" Name="Yellow" HexValue="FFFFFF00" />
      <Color Type="BorderEnabled" Name="Gray" HexValue="FF808080" />
      <Color Type="BorderHighlighted" Name="Gray" HexValue="FF808080" />
    </ColorArray>
  </ControlDescription>
  <ControlDescription Name="Error" Shape="Rectangle" BorderStyle="Raised" BorderSize="2">
